		"@nestjs/swagger": "^7.4.0",
		"@nestjs/websockets": "^10.4.4",
		"@noble/ed25519": "^2.1.0",
		"@sentry/nestjs": "^8.32.0",
		"@sentry/profiling-node": "^8.26.0",
