		"json-canonicalize": "^1.0.6",
		"lint-staged": "^15.2.7",
		"nodemailer": "^6.9.14",
		"otp-generator": "^4.0.1",
		"redoc-express": "^2.1.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"selfsigned": "^2.4.1",
		"socket.io": "^4.8.0",
		"tigerbeetle-node": "^0.16.2",
		"tough-cookie": "^5.0.0",
		"webpack": "^5.94.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.4.4",
		"@nestjs/schematics": "^10.1.4",
		"@nestjs/testing": "^10.4.4",
		"@southlane/cognito-jwt-verifier": "^0.1.8",
